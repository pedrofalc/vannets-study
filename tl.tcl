# set number of nodes
set opt(nn) 37

# set activity file
set opt(af) $opt(config-path)
append opt(af) /activity.tcl

# set mobility file
set opt(mf) $opt(config-path)
append opt(mf) /mobility.tcl

# set start/stop time
set opt(start) 0.0
set opt(stop) 100.0

# set floor size
set opt(x) 4703.98
set opt(y) 5326.84
set opt(min-x) 89.79
set opt(min-y) 3157.35

